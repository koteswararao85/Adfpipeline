{
	"name": "dynamiclinkedsqldbservice",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"parameters": {
			"dbconnection": {
				"type": "string"
			},
			"dbusername": {
				"type": "string"
			},
			"password": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=sqldbserverkoti.database.windows.net;Initial Catalog=sqlserverdbkoti;User ID=@{linkedService().dbusername};Password=@{linkedService().password}"
		}
	}
}